<h1>locations#index</h1>
<% @locations.each do |p| %>
<h2><%= link_to p.title, location_path(p.id) %> </h2>
<%= image_tag p.image.url(:medium) %>
<p> Written by <%= link_to p.user.name, user_path(p.user) %></p>


<h6>Posted <%= time_ago_in_words(p.created_at) %> ago</h6>
<p> <%=  p.body %></p>
<% end %>
